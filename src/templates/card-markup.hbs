{{#each this}}
  <li class='cardContainer'>
    <div class='photo-card'>
      <div class='imgCard'>
        <img class='img-element' src="{{webformatURL}}" data-largesize="{{largeImageURL}}" alt='' />
      </div>

      <div class='stats'>
        <p class='stats-item'>
          <i class='material-icons'>
            thumb_up
          </i>
          {{likes}}
        </p>
        <p class='stats-item'>
          <i class='material-icons'>
            visibility
          </i>
          {{views}}
        </p>
        <p class='stats-item'>
          <i class='material-icons'>
            comment
          </i>
          {{comments}}
        </p>
        <p class='stats-item'>
          <i class='material-icons'>
            cloud_download
          </i>
          {{downloads}}
        </p>
      </div>
    </div>
  </li>
{{/each}}